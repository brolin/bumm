extends ../layout

block content
  h2.display-data <%= model.singularCapitalized %> Detail
            
  table.table.table-bordered
    tr
      th Property
      th Value
<% for(var i in properties) { -%>
        tr
          td <%=properties[i].nameCapitalized %>
          td= <%=model.singular%>.<%=properties[i].name %>
<% } %>

  .form-actions
    a.btn.btn-warning(href="/<%=model.plural%>") Back
    | 
    a.btn.btn-primary(href="/<%=model.plural%>/" + <%=model.singular%>._id + "/edit") Edit
    | 
    a.btn.btn-danger(href="/<%=model.plural%>/" + <%=model.singular%>._id + "/delete") Delete